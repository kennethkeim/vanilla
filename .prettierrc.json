{
   "printWidth": 90,
   "tabWidth": 3,
   "useTabs": false,
   "semi": false,
   "singleQuote": false,
   "quoteProps": "as-needed",
   "jsxSingleQuote": false,
   "trailingComma": "none",
   "bracketSpacing": true,
   "arrowParens": "always",
   "rangeStart": 0,
   "proseWrap": "preserve",
   "htmlWhitespaceSensitivity": "css",
   "endOfLine": "auto"
}
